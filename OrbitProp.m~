function [time_s,lat,long,rmag] = OrbitProp(SMA,ECC,INC,RAAN,AOP,TA)
    %  Returns time from epoch (s), lat (deg), long (deg), earth radius
    %  magnitude given orbital elements SMA, ECC, INC, RAAN, AOP, TA.
    %  Inputs in km and 

    INC = deg2rad(INC); % [rad]

    AOP_0 = deg2rad(AOP_deg_0);
    RAAN_0 = deg2rad(RAAN_deg_0);


    %EARTH DATA
    J2 = 0.00108263;
    pi = 3.14159265358979;
    mu = 398600; % [km3/s2]
    R_e = 6378.14; % [km]
    We_deg = 360.98564735; %deg/day
    We = deg2rad(We_deg);
    lamb_gw_deg_0 = 99.281;
    lamb_gw_0 = deg2rad(lamb_gw_deg_0);

    %CALCS
    T = 1/w_sat;
    a(1) = (mu*(T/(2*pi))^2)^(1/3);

    %Corrected SMA
    diff = 1;
    ii = 1;
    while diff > 1e-6
        dNa = sqrt(mu/(a(ii))^3)*(1+3/2*J2*(R_e/a(ii))^2*(1-e^2)^(-3/2)*...
            (1-3/2*sin(i)^2))-2*pi*w_sat;
        dNda = sqrt(mu)*-1.5*a(ii)^(-2.5)+sqrt(mu)*-3.5*a(ii)^(-4.5)*...
            (1.5*J2*R_e^2*(1-e^2)^(-1.5)*(1-3/2*sin(i)^2));

        a(ii+1) = a(ii) - dNa/dNda;

        diff = abs(a(ii+1)-a(ii));
        ii=ii+1;
    end

    %Loop times
    T_start = clock2frac(10,30,00)+131;
    T_end = clock2frac(10,00,00)+132;
    dt = clock2frac(00,01,00);

    iii=1;
    for T_now = T_start:dt:T_end
    %===================================================
    %          T H I S   B I T   I S   H A 1
    %===================================================
        %Mean Anomaly
        M = M0 + 2*pi*N*(T_now-T_Ep);
        E(1) = M;

        diff = 1;
        ii = 1;
        while diff > 1e-6
            fE = E(ii)-e*sin(E(ii))-M;
            dfE = 1-e*cos(E(ii));

            E(ii+1)=E(ii)-fE/dfE;

            diff = abs(E(ii+1)-E(ii));
            ii = ii+1;
        end
        E=E(end)

        nu(iii) = 2*atan(sqrt(1+e)/sqrt(1-e)*tan(E/2));
        r(iii) = a(end)*(1-e^2)/(1+e*cos(nu(iii)));
    %===================================================
    %          T H I S   B I T   I S   H A 2
    %===================================================

        lamb_gw(iii) = wrapTo2Pi(lamb_gw_0 + We*T_now);

        RAAN(iii) = wrapTo2Pi(RAAN_0 - 3/2*J2*(R_e/a(end))^2*N*2*pi*...
            (1-e^2)^(-2)*cos(i)*(T_now-T_Ep));

        AOP(iii) = wrapTo2Pi(AOP_0 + 3/2*J2*(R_e/a(end))^2*N*2*pi*...
            (1-e^2)^(-2)*(2-5/2*sin(i)^2)*(T_now-T_Ep));

    iii=iii+1;
    end

    %Calc Latitude
    Lat = asin(sin(AOP+nu)*sin(i));

    %Calc Longitude
    lambda_sin = tan(Lat)/tan(i);
    lambda_cos = cos(AOP+nu)./cos(Lat);
    Long1 = atan2(lambda_sin,lambda_cos)+RAAN-lamb_gw;
    Long2 = wrapToPi(Long1);

    nu = wrapTo2Pi(nu);
    nu = rad2deg(nu);

    Lat_deg=rad2deg(Lat);
    Long_deg=rad2deg(Long2);

